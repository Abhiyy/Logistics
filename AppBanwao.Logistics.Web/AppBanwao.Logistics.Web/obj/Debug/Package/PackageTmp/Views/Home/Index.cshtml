    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/Images/air.jpg" style="height:1000px" class="img-responsive center-block" alt="Second Image">
            <div class="carousel-caption text-uppercase carasoul-captionPadding">
                <div class="animated bounceInDown">
                    <h1> Welcome to v-max express</h1>
                </div>
                <div class="animated slideInLeft">
                    <h3> <i class="fa fa-truck"></i> <i class="fa fa-ship"></i> <i class="fa fa-plane"></i></h3>
                </div>
                <div class="animated slideInRight">
                    <h3> <i>Trust the best!!!</i></h3>
                </div>

            </div>
            
        </div>
        <div class="item ">
            <img src="~/Images/sea.jpg" style="height:1000px" class="img-responsive center-block" alt="First Image">
            <div class="carousel-caption text-uppercase carasoul-captionPadding">
                <div class="animated rotateIn">
                    <h1> Deliver Your Goods and Cargo!!</h1>
                </div>
                <div class="animated slideInLeft">
                    <h3> <i class="fa fa-clock-o"></i> 27 <i class="fa fa-close"></i> 7 <i class="fa fa-calendar"></i> 365 days </h3>
                </div>
                <div class="animated slideInRight">
                    <h3> <i>We are here for you!!!</i></h3>
                </div>
            </div>
        </div>
        <div class="item ">
            <img src="~/Images/road.jpg" style="height:1000px" class="img-responsive center-block" alt="Second Image">
            <div class="carousel-caption text-uppercase carasoul-captionPadding">
                <div class="animated rotateIn">
                    <h1> Cost effective shipments!!</h1>
                </div>
                <div class="animated slideInLeft">
                    <h3> <i class="fa fa-inr"></i> </h3>
                </div>
                <div class="animated slideInRight">
                    <h3> <i>We are provide unique way of reducing your delivery costs !!</i></h3>
                </div>
            </div>
        </div>

        
        
